/*Omar Velasco Guzmán 2C*/
/*Busqueda secuencial*/

var vector=[5,2,4,3,1];
var dato,i;
var band='F';

dato=4;

i=0;
while((band=='F') && (i<5)){
    if(vector[i]==dato){
        band='V';
    }
    i++;
}

if(band=='F'){
    console.log("El numero a buscar no fue encontrado");
}
else if(band=='V'){
        console.log("El numero si fue encontrado en la posición"+ i-1);
}
